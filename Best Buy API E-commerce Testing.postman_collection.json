          "script": {
            "id": "d34e8d6e-2c88-4c0a-9dfa-a9f48da5c99e",
            "type": "text/javascript",
            "packages": {},
            "requests": {},
            "exec": [
              "if (!pm.environment.get(\"url\")) {\r",
              "    throw new Error(\"baseUrl is not set in environment\");\r",
              "}\r",
              "// Headers\r",
              "pm.request.headers.upsert({\r",
              "    key: \"Content-Type\",\r",
              "    value: \"application/json\"\r",
              "});\r",
              "\r",
              "// Get token from environment or collection variable\r",
              "let token = pm.variables.get(\"token\");\r",
              "\r",
              "if (!token) {\r",
              "    console.log(\"Token is missing!\");\r",
              "    throw new Error(\"Authorization token is not set in {{token}} variable.\");\r",
              "}\r",
              "\r",
              "console.log(\"Token found:\", token);\r",
              "// Tracking\r",
              "pm.environment.set(\"requestTime\", new Date().toISOString());"
            ]
          }
